<config>
  <native xmlns="http://cisco.com/ns/yang/ned/ios">
    <interface>
      <GigabitEthernet>
        <name>3</name>
        <bandwidth>
          <kilobits>100000</kilobits>
        </bandwidth>
        <service-policy>
          <output>POLICY-TRANSPORT-1-SHAPE-ONLY</output>
        </service-policy>
      </GigabitEthernet>
      <Tunnel>
        <name>10</name>
        <ip>
          <nhrp>
            <map>
              <group>
                <name>RS-GROUP-100MBPS</name>
                <service-policy>
                  <output>RS-GROUP-100MBPS-POLICY</output>
                </service-policy>
              </group>
              <group>
                <name>RS-GROUP-10MBPS</name>
                <service-policy>
                  <output>RS-GROUP-10MBPS-POLICY</output>
                </service-policy>
              </group>
              <group>
                <name>RS-GROUP-200MBPS</name>
                <service-policy>
                  <output>RS-GROUP-200MBPS-POLICY</output>
                </service-policy>
              </group>
              <group>
                <name>RS-GROUP-20MBPS</name>
                <service-policy>
                  <output>RS-GROUP-20MBPS-POLICY</output>
                </service-policy>
              </group>
              <group>
                <name>RS-GROUP-300MBPS</name>
                <service-policy>
                  <output>RS-GROUP-300MBPS-POLICY</output>
                </service-policy>
              </group>
              <group>
                <name>RS-GROUP-30MBPS</name>
                <service-policy>
                  <output>RS-GROUP-30MBPS-POLICY</output>
                </service-policy>
              </group>
              <group>
                <name>RS-GROUP-4G</name>
                <service-policy>
                  <output>!!!@</output>
                </service-policy>
              </group>
              <group>
                <name>RS-GROUP-50MBPS</name>
                <service-policy>
                  <output>RS-GROUP-50MBPS-POLICY</output>
                </service-policy>
              </group>
            </map>
          </nhrp>
        </ip>
      </Tunnel>
    </interface>
    <class-map>
      <name>CALL-SIGNALING</name>
      <prematch>match-any</prematch>
      <match>
        <dscp>cs3</dscp>
      </match>
    </class-map>
    <class-map>
      <name>CRITICAL-DATA</name>
      <prematch>match-any</prematch>
      <match>
        <dscp>af11</dscp>
        <dscp>af12</dscp>
        <dscp>af13</dscp>
        <dscp>cs2</dscp>
        <dscp>af21</dscp>
        <dscp>af22</dscp>
        <dscp>af23</dscp>
      </match>
    </class-map>
    <class-map>
      <name>INTERACTIVE-VIDEO</name>
      <prematch>match-any</prematch>
      <match>
        <dscp>cs4</dscp>
        <dscp>af41</dscp>
        <dscp>af42</dscp>
        <dscp>af43</dscp>
      </match>
    </class-map>
    <class-map>
      <name>NET-CTRL</name>
      <prematch>match-any</prematch>
      <match>
        <dscp>cs6</dscp>
      </match>
    </class-map>
    <class-map>
      <name>SCAVENGER</name>
      <prematch>match-any</prematch>
      <match>
        <dscp>cs1</dscp>
      </match>
    </class-map>
    <class-map>
      <name>STREAMING-VIDEO</name>
      <prematch>match-any</prematch>
      <match>
        <dscp>af31</dscp>
        <dscp>af32</dscp>
        <dscp>af33</dscp>
        <dscp>cs5</dscp>
      </match>
    </class-map>
    <class-map>
      <name>VOICE</name>
      <prematch>match-any</prematch>
      <match>
        <dscp>ef</dscp>
      </match>
    </class-map>
    <policy-map>
      <name>POLICY-TRANSPORT-1-SHAPE-ONLY</name>
      <class>
        <name>class-default</name>
        <action-list>
          <action-type>shape</action-type>
          <shape>
            <average>
              <bit-rate>100000000</bit-rate>
            </average>
          </shape>
        </action-list>
      </class>
    </policy-map>
    <policy-map>
      <name>RS-GROUP-100MBPS-POLICY</name>
      <class>
        <name>class-default</name>
        <action-list>
          <action-type>shape</action-type>
          <shape>
            <average>
              <bit-rate>100000000</bit-rate>
            </average>
          </shape>
        </action-list>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <ratio>100</ratio>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>service-policy</action-type>
          <service-policy>WAN</service-policy>
        </action-list>
      </class>
    </policy-map>
    <policy-map>
      <name>RS-GROUP-10MBPS-POLICY</name>
      <class>
        <name>class-default</name>
        <action-list>
          <action-type>shape</action-type>
          <shape>
            <average>
              <bit-rate>10000000</bit-rate>
            </average>
          </shape>
        </action-list>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <ratio>10</ratio>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>service-policy</action-type>
          <service-policy>WAN</service-policy>
        </action-list>
      </class>
    </policy-map>
    <policy-map>
      <name>RS-GROUP-200MBPS-POLICY</name>
      <class>
        <name>class-default</name>
        <action-list>
          <action-type>shape</action-type>
          <shape>
            <average>
              <bit-rate>200000000</bit-rate>
            </average>
          </shape>
        </action-list>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <ratio>200</ratio>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>service-policy</action-type>
          <service-policy>WAN</service-policy>
        </action-list>
      </class>
    </policy-map>
    <policy-map>
      <name>RS-GROUP-20MBPS-POLICY</name>
      <class>
        <name>class-default</name>
        <action-list>
          <action-type>shape</action-type>
          <shape>
            <average>
              <bit-rate>20000000</bit-rate>
            </average>
          </shape>
        </action-list>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <ratio>20</ratio>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>service-policy</action-type>
          <service-policy>WAN</service-policy>
        </action-list>
      </class>
    </policy-map>
    <policy-map>
      <name>RS-GROUP-300MBPS-POLICY</name>
      <class>
        <name>class-default</name>
        <action-list>
          <action-type>shape</action-type>
          <shape>
            <average>
              <bit-rate>300000000</bit-rate>
            </average>
          </shape>
        </action-list>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <ratio>300</ratio>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>service-policy</action-type>
          <service-policy>WAN</service-policy>
        </action-list>
      </class>
    </policy-map>
    <policy-map>
      <name>RS-GROUP-30MBPS-POLICY</name>
      <class>
        <name>class-default</name>
        <action-list>
          <action-type>shape</action-type>
          <shape>
            <average>
              <bit-rate>30000000</bit-rate>
            </average>
          </shape>
        </action-list>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <ratio>30</ratio>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>service-policy</action-type>
          <service-policy>WAN</service-policy>
        </action-list>
      </class>
    </policy-map>
    <policy-map>
      <name>RS-GROUP-4G-POLICY</name>
      <class>
        <name>class-default</name>
        <action-list>
          <action-type>shape</action-type>
          <shape>
            <average>
              <bit-rate>8000000</bit-rate>
            </average>
          </shape>
        </action-list>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <ratio>8</ratio>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>service-policy</action-type>
          <service-policy>WAN</service-policy>
        </action-list>
      </class>
    </policy-map>
    <policy-map>
      <name>RS-GROUP-50MBPS-POLICY</name>
      <class>
        <name>class-default</name>
        <action-list>
          <action-type>shape</action-type>
          <shape>
            <average>
              <bit-rate>50000000</bit-rate>
            </average>
          </shape>
        </action-list>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <ratio>50</ratio>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>service-policy</action-type>
          <service-policy>WAN</service-policy>
        </action-list>
      </class>
    </policy-map>
    <policy-map>
      <name>WAN</name>
      <class>
        <name>CALL-SIGNALING</name>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <percent>4</percent>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>set</action-type>
          <set>
            <dscp>
              <tunnel/>
              <dscp-val>af21</dscp-val>
            </dscp>
          </set>
        </action-list>
      </class>
      <class>
        <name>CRITICAL-DATA</name>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <percent>25</percent>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>random-detect</action-type>
          <random-detect>
            <dscp-based/>
          </random-detect>
        </action-list>
        <action-list>
          <action-type>set</action-type>
          <set>
            <dscp>
              <tunnel/>
              <dscp-val>af21</dscp-val>
            </dscp>
          </set>
        </action-list>
      </class>
      <class>
        <name>INTERACTIVE-VIDEO</name>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <percent>30</percent>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>random-detect</action-type>
          <random-detect>
            <dscp-based/>
          </random-detect>
        </action-list>
        <action-list>
          <action-type>set</action-type>
          <set>
            <dscp>
              <tunnel/>
              <dscp-val>af41</dscp-val>
            </dscp>
          </set>
        </action-list>
      </class>
      <class>
        <name>NET-CTRL</name>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <percent>5</percent>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>set</action-type>
          <set>
            <dscp>
              <tunnel/>
              <dscp-val>cs6</dscp-val>
            </dscp>
          </set>
        </action-list>
      </class>
      <class>
        <name>SCAVENGER</name>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <percent>1</percent>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>set</action-type>
          <set>
            <dscp>
              <tunnel/>
              <dscp-val>af11</dscp-val>
            </dscp>
          </set>
        </action-list>
      </class>
      <class>
        <name>STREAMING-VIDEO</name>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <percent>10</percent>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>random-detect</action-type>
          <random-detect>
            <dscp-based/>
          </random-detect>
        </action-list>
        <action-list>
          <action-type>set</action-type>
          <set>
            <dscp>
              <tunnel/>
              <dscp-val>af31</dscp-val>
            </dscp>
          </set>
        </action-list>
      </class>
      <class>
        <name>VOICE</name>
        <action-list>
          <action-type>priority</action-type>
          <priority>
            <level>1</level>
          </priority>
        </action-list>
        <action-list>
          <action-type>police</action-type>
          <police-cir-percent>
            <police>
              <cir>
                <percent>
                  <percentage>10</percentage>
                </percent>
              </cir>
            </police>
          </police-cir-percent>
        </action-list>
        <action-list>
          <action-type>set</action-type>
          <set>
            <dscp>
              <tunnel/>
              <dscp-val>ef</dscp-val>
            </dscp>
          </set>
        </action-list>
      </class>
      <class>
        <name>class-default</name>
        <action-list>
          <action-type>bandwidth</action-type>
          <bandwidth>
            <remaining>
              <percent>25</percent>
            </remaining>
          </bandwidth>
        </action-list>
        <action-list>
          <action-type>random-detect</action-type>
        </action-list>
      </class>
    </policy-map>
  </native>
</config>