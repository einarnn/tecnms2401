<?xml version="1.0" encoding="utf-8"?>
<rpc xmlns="urn:ietf:params:xml:ns:netconf:base:1.0" message-id="${TIMESTAMP}">
  <edit-config>
    <target>
      <candidate/>
    </target>
    <error-option>rollback-on-error</error-option>
    <config>
      <vrfs xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-infra-rsi-cfg">
        <vrf>
          <vrf-name>RED</vrf-name>
          <create/>
          <afs>
            <af>
              <af-name>ipv4</af-name>
              <saf-name>unicast</saf-name>
              <topology-name>default</topology-name>
              <create/>
              <bgp xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-ipv4-bgp-cfg">
                <import-route-targets>
                  <route-targets>
                    <route-target>
                      <type>as</type>
                      <as-or-four-byte-as>
                        <as-xx>0</as-xx>
                        <as>65172</as>
                        <as-index>0</as-index>
                        <stitching-rt>0</stitching-rt>
                      </as-or-four-byte-as>
                    </route-target>
                  </route-targets>
                </import-route-targets>
                <export-route-targets>
                  <route-targets>
                    <route-target>
                      <type>as</type>
                      <as-or-four-byte-as>
                        <as-xx>0</as-xx>
                        <as>65172</as>
                        <as-index>0</as-index>
                        <stitching-rt>0</stitching-rt>
                      </as-or-four-byte-as>
                    </route-target>
                  </route-targets>
                </export-route-targets>
              </bgp>
            </af>
          </afs>
        </vrf>
      </vrfs>
    </config>
  </edit-config>
</rpc>
